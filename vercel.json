{
  "version": 2,
  "builds": [
    {
      "src": "src/service/index.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm install && npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/service/index.ts"
    }
  ]
} 